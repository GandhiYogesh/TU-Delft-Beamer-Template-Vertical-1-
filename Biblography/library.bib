doi = {10.1007/s00170-005-0387-0},
file = {:C\:/Users/ygandhi1/Dropbox/FRC_TO_Article_Review/Papers/FiberReinfrocedComposite_TopologyOptimization/Topology_Optimization_Algorithms/Method Of Moving Asymptotes family/Study of key algorithms in topology optimization.pdf:pdf},
issn = {02683768},
journal = {Int. J. Adv. Manuf. Technol.},
keywords = {Convolution integral factor method,Hybrid solution algorithm,MMA series algorithms,Numerical instability,Topology optimization},
number = {7-8},
pages = {787--796},
title = {{Study of key algorithms in topology optimization}},
volume = {32},
year = {2007}
}
