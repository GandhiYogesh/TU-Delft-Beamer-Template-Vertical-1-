keywords = {Additive manufacturing,Deposition path planning,Level set,Structural topology optimization},
number = {5},
pages = {930--942},
title = {{Concurrent deposition path planning and structural topology optimization for additive manufacturing}},
volume = {23},
year = {2017}
}
