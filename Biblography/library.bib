issn = {16151488},
journal = {Struct. Multidiscip. Optim.},
keywords = {Anisotropic lattices,Lattice structures,Multi-material,Topology optimization},
number = {4},
pages = {1653--1668},
title = {{Topology optimization of lattices with anisotropic struts}},
volume = {63},
year = {2021}
