title = {{Three dimensional topology optimization with orthotropic material orientation design for additive manufacturing structures}},
url = {https://www.researchgate.net/publication/328580375_Three_dimensional_topology_optimization_with_orthotropic_material_orientation_design_for_additive_manufacturing_structures},
