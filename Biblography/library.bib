issn = {16151488},
journal = {Struct. Multidiscip. Optim.},
keywords = {Feature-mapping,Fixed-grid,High-level design,Structural optimization},
number = {4},
pages = {1597--1638},
publisher = {Structural and Multidisciplinary Optimization},
title = {{A review on feature-mapping methods for structural optimization}},
volume = {62},
